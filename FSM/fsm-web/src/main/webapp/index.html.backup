		<aside class="al-sidebar" ng-swipe-right="$baSidebarService.setMenuCollapsed(false)" ng-swipe-left="$baSidebarService.setMenuCollapsed(true)"
		       ng-mouseleave="hoverElemTop=selectElemTop">
		  <ul class="al-sidebar-list" slimscroll="{height: '{{menuHeight}}px'}" slimscroll-watch="menuHeight" >
		    <li ng-repeat="item in ::modules" class="al-sidebar-list-item dropdown"
		        ng-class="::{'with-sub-menu': item.submenus}" ui-sref-active="selected"
		        ba-sidebar-toggling-item="item">
		
		      <a ng-if="::!item.submenus" ng-mouseenter="hoverItem($event, item)"  ui-state="item.stateRef || ''" ng-href="#{{::(item.path ? item.path: '')}}" class="al-sidebar-list-link">
		        <i class="{{::item.icon }}"></i><span ng-Bind="::item.name"></span>
		      </a>
		      
		      <a ng-if="::item.submenus" ng-mouseenter="hoverItem($event, item)" ng-click="openMenu($event, item.submenus)" ui-state="item.stateRef || ''" ng-href="#{{::(item.path ? item.path: '')}}" class="al-sidebar-list-link">
		        <i class="{{ ::item.icon }}"></i><span ng-Bind="::item.name"></span>
		        <b ng-class="::{'fa fa-angle-down': item.submenus}" ng-if="::item.submenus"></b>
		      </a>
			  <ul ng-if="::item.submenus" class="al-sidebar-sublist" ng-class="{'slide-right': item.slideRight}">
					<li ng-repeat="submenu in item.submenus"><a ng-href="#{{submenu.path}}">{{::submenu.name}}</a></li>
              </ul>
		    </li>
		  </ul>
		  <div class="sidebar-hover-elem" ng-style="{top: hoverElemTop + 'px', height: hoverElemHeight + 'px'}"
		       ng-class="{'show-hover-elem': showHoverElem }"></div>
		</aside>

